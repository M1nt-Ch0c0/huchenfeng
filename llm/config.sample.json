{
  "providers": [
    {
      "name": "openai",
      "driver": "openai_chat",
      "base_url": "https://api.openai.com/v1/chat/completions",
      "timeout": 60,
      "default_params": {
        "temperature": 0.3,
        "max_tokens": 1024
      }, 
      "models": [
        {
          "name": "gpt-4o-mini",
          "keys": [
            "$ENV:OPENAI_API_KEY",
            "$ENV:OPENAI_FALLBACK_KEY"
          ],
          "jsonable": true,
          "params": {
            "temperature": 0.2
          }
        },
        {
          "name": "gpt-4.1-mini",
          "keys": [
            "$ENV:OPENAI_API_KEY"
          ],
          "jsonable": true
        }
      ]
    },
    {
      "name": "deepseek",
      "driver": "deepseek_chat",
      "base_url": "https://api.deepseek.com/v1/chat/completions",
      "timeout": 60,
      "default_params": {
        "temperature": 0.5
      },
      "models": [
        {
          "name": "deepseek-chat",
          "keys": [
            "$ENV:DEEPSEEK_API_KEY",
            "$ENV:DEEPSEEK_BACKUP_KEY"
          ],
          "jsonable": false
        },
        {
          "name": "deepseek-reasoner",
          "keys": [
            "$ENV:DEEPSEEK_API_KEY"
          ],
          "jsonable": true,
          "params": {
            "max_tokens": 2048
          }
        }
      ]
    }, {
      "name": "openrouter",
      "driver": "openrouter_chat",
      "base_url": "https://api.openrouter.com/v1/chat/completions",
      "timeout": 60,
      "default_params": {
        "temperature": 0.3,
        "max_tokens": 1024
      }, 
      "models": [
        {
          "name": "deepseek/deepseek-chat-v3.1:free",
          "keys": [
            "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"
          ],
          "jsonable": true,
          "params": {
            "temperature": 0.3
          }
        },
        {
          "name": "deepseek/deepseek-reasoner-v3.1:free",
          "keys": [
            "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"
          ],
          "jsonable": true
        }
        
      ]
    }
  ]
}
